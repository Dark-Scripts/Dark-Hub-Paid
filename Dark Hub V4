function CorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Correct Key!",
        Content = "You have entered the correct key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function IncorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Incorrect Key!",
        Content = "You have entered the incorrect key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      	if _G.KeyInput == _G.Key then
            if game.PlaceId == 286090429 then
                MakeScriptHub1()
            elseif game.PlaceId == 3233893879 then
                MakeScriptHub2()
            elseif game.PlaceId == 3527629287 then
                MakeScriptHub3()
            elseif game.PlaceId == 10705886260 then
                MakeScriptHub4()
            elseif game.PlaceId == 8962384769 then
                MakeScriptHub5()
            elseif game.PlaceId == 621129760 then
                MakeScriptHub6()
            elseif game.PlaceId == 6403373529 then
                MakeScriptHub7()
            elseif game.PlaceId == 2377868063 then
                MakeScriptHub8()
            elseif game.PlaceId == 10594623896 then
	        MakeScriptHub9()
            end
        end
  	end    
})
